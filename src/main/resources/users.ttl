@prefix :      <http://www.infrabim.nl/bimbots-psd-repository/users#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .

:User   a                owl:Class ;
        rdfs:subClassOf  owl:Thing .

<http://www.infrabim.nl/bimbots-psd-repository/users>
        a                owl:Ontology ;
        owl:versionInfo  "Created with TopBraid Composer" .

:salt   a            owl:FunctionalProperty , owl:DatatypeProperty ;
        rdfs:domain  :User ;
        rdfs:range   xsd:string .

:email  a            owl:FunctionalProperty , owl:DatatypeProperty ;
        rdfs:domain  :User ;
        rdfs:range   xsd:string .

:password  a         owl:FunctionalProperty , owl:DatatypeProperty ;
        rdfs:domain  :User ;
        rdfs:range   xsd:string .

:name   a            owl:FunctionalProperty , owl:DatatypeProperty ;
        rdfs:domain  :User ;
        rdfs:range   xsd:string .
